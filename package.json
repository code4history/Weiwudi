{
  "name": "@c4h/weiwudi",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.5",
  "description": "Service worker for tile cache",
  "main": "dist/weiwudi.umd.js",
  "module": "dist/weiwudi.es.js",
  "types": "dist/weiwudi.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/weiwudi.d.ts",
      "import": "./dist/weiwudi.es.js",
      "require": "./dist/weiwudi.umd.js"
    },
    "./sw": {
      "types": "./dist/weiwudi_gw.d.ts",
      "import": "./dist/weiwudi-sw.es.js"
    }
  },
  "scripts": {
    "test": "playwright test",
    "dev": "vite --host",
    "build": "vite build -c vite.config.lib.js && vite build -c vite.config.sw-es.js && vite build -c vite.config.sw-umd.js && node -e \"import('fs').then(fs => fs.copyFileSync('dist/weiwudi-sw.umd.js', 'public/weiwudi-sw.js'))\"",
    "build:demo": "pnpm run build && vite build -c vite.config.demo.js",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Weiwudi.git"
  },
  "keywords": [
    "Service",
    "Worker",
    "Tile",
    "Cache"
  ],
  "author": "Kohei Otsuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code4history/Weiwudi/issues"
  },
  "homepage": "https://github.com/code4history/Weiwudi#readme",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "workbox-build": "7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-window": "7.4.0"
  },
  "peerDependencies": {
    "workbox-routing": "^7.4.0"
  },
  "peerDependenciesMeta": {
    "workbox-routing": {
      "optional": false
    }
  }
}